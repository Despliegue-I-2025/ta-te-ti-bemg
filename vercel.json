{
  "version": 2,
  "name": "tateti-bemg",
  "builds": [
    {
      "src": "app/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "app/server.js"
    }
  ],
  "functions": {
    "app/server.js": {
      "maxDuration": 10
    }
  },
  "env": {
    "NODE_ENV": "production"
  }
}
